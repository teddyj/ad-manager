{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "regions": ["sfo1"],
  "env": {
    "VITE_NEW_CAMPAIGN_FLOW": "true",
    "VITE_CREATIVE_AI": "true"
  }
} 